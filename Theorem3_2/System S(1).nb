Cell["\<\
We use the function \[OpenCurlyDoubleQuote]Twist[]\[CloseCurlyDoubleQuote] \
for doing a twist if u=0 is a characteristic direction of the system, and the \
function \[OpenCurlyDoubleQuote]UVertical[]\[CloseCurlyDoubleQuote] for doing \
a blow-up with a rescaling of the time.\
\>", "Text",
 CellChangeTimes->{{3.9688372394395*^9, 3.968837322276725*^9}, {
  3.968848411045354*^9, 3.968848413498434*^9}, {3.968848448048265*^9, 
  3.968848490726829*^9}},ExpressionUUID->"8e94d38f-872e-4337-980d-\
c5e015ad643f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Twist", "=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xp", ",", "yp", ",", "x", ",", "y", ",", "x1", ",", "y1"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"xp", "+", "yp"}], ",", "yp"}], "}"}], "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "->", 
          RowBox[{"x1", "-", "y1"}]}], ",", 
         RowBox[{"y", "->", "y1"}]}], "}"}]}], "//", "Factor"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.91803883885958*^9, 3.918038931529788*^9}},
 CellLabel->"In[14]:=",ExpressionUUID->"69e960d6-95b3-4fdf-ad95-2b0762f56702"],

Cell[BoxData[
 RowBox[{
  RowBox[{"UVertical", "=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xp1", ",", "yp1", ",", "x1", ",", "y1", ",", "x2", ",", "y2"}],
       "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"xp1", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"x1", " ", "yp1"}], "-", 
              RowBox[{"y1", " ", "xp1"}]}], ")"}], "/", 
            RowBox[{"x1", "^", "2"}]}]}], "}"}], "/.", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"x1", "->", "x2"}], ",", 
           RowBox[{"y1", "->", 
            RowBox[{"x2", " ", "y2"}]}]}], "}"}]}], ")"}], "/", "x2"}], "//", 
      "Factor"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.918038751962864*^9, 3.918038797841317*^9}, 
   3.91803885335767*^9, {3.918038934857078*^9, 3.91803902018296*^9}, {
   3.9182015820551357`*^9, 3.918201582860316*^9}},
 CellLabel->"In[15]:=",ExpressionUUID->"0443348e-4abd-4101-99c9-a1cf99b950a8"],

