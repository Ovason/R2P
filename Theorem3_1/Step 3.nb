     SuperscriptBox["k1", "2"], " ", "x"}], "+", 
    RowBox[{"4", " ", 
     SuperscriptBox["a1", "3"], " ", 
     SuperscriptBox["k1", "3"], " ", "x"}], "+", 
    RowBox[{"4", " ", "k1", " ", 
     SuperscriptBox["x", "2"]}], "+", 
    RowBox[{"8", " ", "a1", " ", 
     SuperscriptBox["k1", "2"], " ", 
     SuperscriptBox["x", "2"]}], "+", 
    RowBox[{"4", " ", 
     SuperscriptBox["a1", "2"], " ", 
     SuperscriptBox["k1", "3"], " ", 
     SuperscriptBox["x", "2"]}], "+", 
    RowBox[{"2", " ", 
     SuperscriptBox["k1", "2"], " ", 
     SuperscriptBox["x", "3"]}], "-", 
    RowBox[{"k1", " ", "y"}], "+", 
    RowBox[{"a1", " ", 
     SuperscriptBox["k1", "2"], " ", "y"}], "+", 
    RowBox[{"2", " ", 
