Cell["We compute the extactic polynomial E1 of the system", "Text",
 CellChangeTimes->{{3.9685545053191032`*^9, 
  3.96855450723289*^9}},ExpressionUUID->"615863b8-7611-4418-a75b-\
5fb6d32f6c7c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"E1", "=", 
  RowBox[{
   RowBox[{
    RowBox[{"xp", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"yp", ",", "x"}], "]"}], "xp"}], "+", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"yp", ",", "y"}], "]"}], "yp"}]}], ")"}]}], "-", 
    RowBox[{"yp", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"xp", ",", "x"}], "]"}], "xp"}], "+", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{"xp", ",", "y"}], "]"}], "yp"}]}], ")"}]}]}], "//", 
   "Factor"}]}]], "Input",
 CellChangeTimes->{{3.968554785201695*^9, 3.9685547857998877`*^9}, {
  3.9685548490633307`*^9, 3.968554850612187*^9}},
 CellLabel->"In[53]:=",ExpressionUUID->"4a9dd176-d474-460e-a2a6-d54bf9b4357c"],

Cell[BoxData[
 RowBox[{"2", " ", 
  RowBox[{"(", 
   RowBox[{
    SuperscriptBox["a1", "2"], "+", 
    RowBox[{"2", " ", "a1", " ", "x"}], "+", "y"}], ")"}], " ", 
  RowBox[{"(", 
   RowBox[{"a1", "+", 
    RowBox[{"3", " ", 
     SuperscriptBox["a1", "2"], " ", "k1"}], "+", 
    RowBox[{"3", " ", 
     SuperscriptBox["a1", "3"], " ", 
     SuperscriptBox["k1", "2"]}], "+", 
    RowBox[{
     SuperscriptBox["a1", "4"], " ", 
     SuperscriptBox["k1", "3"]}], "+", "x", "+", 
    RowBox[{"6", " ", "a1", " ", "k1", " ", "x"}], "+", 
    RowBox[{"9", " ", 
